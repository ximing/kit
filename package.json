{
  "name": "@rabjs/kit",
  "version": "0.0.1",
  "description": "A comprehensive TypeScript utility library with 100+ helper functions for arrays, objects, strings, functions, numbers, dates, promises, and more. Similar to lodash but with full TypeScript support and tree-shaking friendly.",
  "author": "TypeStack contributors",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ximing/kit.git"
  },
  "tags": [
    "utility",
    "toolkit",
    "lodash",
    "helpers",
    "typescript",
    "array",
    "object",
    "string",
    "function",
    "promise",
    "date",
    "collection"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:clean": "rimraf dist",
    "build:cjs": "rollup -c rollup.config.cjs.js",
    "build:esm": "rollup -c rollup.config.esm.js",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "prettier:fix": "prettier --write \"**/*.{ts,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,md}\"",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts src/",
    "lint:check": "eslint --max-warnings 0 --ext .ts src/",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --coverage --verbose",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{json,md}": [
      "pnpm run prettier:fix"
    ],
    "*.ts": [
      "pnpm run lint:fix",
      "pnpm run prettier:fix"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "readline": "^1.3.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./array": {
      "types": "./dist/types/array/index.d.ts",
      "import": "./dist/esm/array/index.js",
      "require": "./dist/cjs/array/index.js"
    },
    "./object": {
      "types": "./dist/types/object/index.d.ts",
      "import": "./dist/esm/object/index.js",
      "require": "./dist/cjs/object/index.js"
    },
    "./string": {
      "types": "./dist/types/string/index.d.ts",
      "import": "./dist/esm/string/index.js",
      "require": "./dist/cjs/string/index.js"
    },
    "./function": {
      "types": "./dist/types/function/index.d.ts",
      "import": "./dist/esm/function/index.js",
      "require": "./dist/cjs/function/index.js"
    },
    "./number": {
      "types": "./dist/types/number/index.d.ts",
      "import": "./dist/esm/number/index.js",
      "require": "./dist/cjs/number/index.js"
    },
    "./is": {
      "types": "./dist/types/is/index.d.ts",
      "import": "./dist/esm/is/index.js",
      "require": "./dist/cjs/is/index.js"
    },
    "./date": {
      "types": "./dist/types/date/index.d.ts",
      "import": "./dist/esm/date/index.js",
      "require": "./dist/cjs/date/index.js"
    },
    "./promise": {
      "types": "./dist/types/promise/index.d.ts",
      "import": "./dist/esm/promise/index.js",
      "require": "./dist/cjs/promise/index.js"
    },
    "./collection": {
      "types": "./dist/types/collection/index.d.ts",
      "import": "./dist/esm/collection/index.js",
      "require": "./dist/cjs/collection/index.js"
    },
    "./math": {
      "types": "./dist/types/math/index.d.ts",
      "import": "./dist/esm/math/index.js",
      "require": "./dist/cjs/math/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}